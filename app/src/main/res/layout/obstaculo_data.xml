    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="#ffffffff">
    <include layout="@layout/obstaculo_prompt"/>
    <LinearLayout
        android:orientation="vertical"
        android:gravity="center_horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
